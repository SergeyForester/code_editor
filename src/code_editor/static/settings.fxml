<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="settingsContainer" maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SettingsController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Theme">
            <font>
                <Font name="Calibri" size="17.0" />
            </font>
        </Label>
        <ComboBox layoutX="14.0" layoutY="35.0" fx:id="themeComboBox" onAction="#selectThemeAction" prefHeight="25.0" prefWidth="89.0" promptText="Theme">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Dracula" />
                    <String fx:value="Default" />
                </FXCollections>
            </items>
        </ComboBox>
    </children>
</AnchorPane>
